{
  "publisher": "simonhe",
  "name": "lazy-install",
  "displayName": "lazy-install",
  "version": "0.0.9",
  "packageManager": "pnpm@8.15.9",
  "description": "lazy-install",
  "author": "Simon He <https://github.com/Simon-He95>",
  "license": "MIT",
  "funding": "https://github.com/sponsors/Simon-He95",
  "homepage": "https://github.com/Simon-He95/lazy-install#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/Simon-He95/lazy-install"
  },
  "bugs": {
    "url": "https://github.com/Simon-He95/lazy-install/issues"
  },
  "sponsor": {
    "url": "https://github.com/Simon-He95/sponsor"
  },
  "categories": [
    "Other"
  ],
  "main": "./dist/index.js",
  "icon": "icon.png",
  "files": [
    "dist"
  ],
  "engines": {
    "vscode": "^1.95.0"
  },
  "activationEvents": [
    "onStartupFinished"
  ],
  "contributes": {
    "configuration": {
      "type": "object",
      "title": "lazy-install",
      "properties": {
        "lazy-install.way": {
          "type": "string",
          "default": "ni",
          "description": "Choose how to install it, such as ni, npm, pnpm, yarn, default: ni"
        }
      }
    }
  },
  "scripts": {
    "dev": "pnpm build --watch",
    "test": "vitest",
    "build": "tsup src/index.ts --external vscode",
    "pack": "vsce package --no-dependencies",
    "lint": "eslint . --cache",
    "lint:fix": "eslint . --fix",
    "publish": "vsce publish --no-dependencies",
    "typecheck": "tsc --noEmit",
    "release": "bumpp && pnpm run publish"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.27.3",
    "@simon_he/eslint-config": "^0.0.6",
    "@types/node": "^18.19.64",
    "@types/vscode": "^1.95.0",
    "@vscode-use/utils": "^0.1.36",
    "bumpp": "^9.8.1",
    "eslint": "^8.57.1",
    "esno": "^0.16.3",
    "find-up": "^7.0.0",
    "lazy-js-utils": "^0.1.18",
    "pnpm": "^8.15.9",
    "rimraf": "^4.4.1",
    "tsup": "^6.7.0",
    "typescript": "^5.6.3",
    "vitest": "^0.29.8",
    "vsce": "^2.15.0",
    "yamljs": "^0.3.0"
  }
}
